{
    "name": "amazon-invoices/pdf-ocr-processor",
    "description": "PDF OCR processing library for Amazon invoices using Tesseract",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Amazon Invoices Team",
            "email": "dev@example.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "ext-gd": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phpstan/phpstan": "^1.0"
    },
    "suggest": {
        "ext-imagick": "For better image processing performance"
    },
    "autoload": {
        "psr-4": {
            "AmazonInvoices\\PdfProcessor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AmazonInvoices\\PdfProcessor\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
