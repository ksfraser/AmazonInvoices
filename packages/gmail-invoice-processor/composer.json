{
    "name": "amazon-invoices/gmail-invoice-processor",
    "description": "Gmail-based invoice processing library for Amazon invoices",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Amazon Invoices Team",
            "email": "dev@example.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "google/apiclient": "^2.0",
        "ext-json": "*",
        "ext-openssl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "AmazonInvoices\\GmailProcessor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AmazonInvoices\\GmailProcessor\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
